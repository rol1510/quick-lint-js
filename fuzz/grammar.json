{
  "productions": [
    {
      "name": "StmtList",
      "rules": [
        { "bias": 2.0, "rule": ["Stmt", "StmtList"] },
        { "bias": 1.0, "rule": ["Stmt"] }
      ],
      "default": "::/* default StmtList */"
    },
    {
      "name": "Stmt",
      "rules": [{ "bias": 1.0, "rule": ["Expr", "::;\\n"] }],
      "default": "::/* default Stmt */"
    },
    {
      "name": "Expr",
      "rules": [
        { "bias": 1, "rule": ["Expr", "Operator", "Expr"] },
        { "bias": 1.5, "rule": ["Value"] }
      ],
      "default": "::12"
    },
    {
      "name": "Value",
      "rules": [
        { "bias": 1.0, "rule": ["::1"] },
        { "bias": 1.0, "rule": ["::\"String\""] },
        { "bias": 1.0, "rule": ["::true"] },
        { "bias": 1.0, "rule": ["::3.", "::141"] }
      ],
      "default": "::1"
    },
    {
      "name": "Operator",
      "rules": [
        { "bias": 1.0, "rule": ["::+"] },
        { "bias": 1.0, "rule": ["::-"] },
        { "bias": 1.0, "rule": ["::*"] },
        { "bias": 1.0, "rule": ["::/"] },
        { "bias": 1.0, "rule": ["::%"] },
        { "bias": 1.0, "rule": ["::=="] },
        { "bias": 1.0, "rule": ["::!="] },
        { "bias": 1.0, "rule": ["::<"] },
        { "bias": 1.0, "rule": ["::>"] },
        { "bias": 1.0, "rule": ["::<="] },
        { "bias": 1.0, "rule": ["::>="] },
        { "bias": 1.0, "rule": ["::&&"] },
        { "bias": 1.0, "rule": ["::||"] }
      ],
      "default": "::+"
    }
  ]
}
