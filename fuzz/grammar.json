{
  "productions": [
    {
      "name": "StmtList",
      "rules": [["Stmt", "StmtList"], ["Stmt", "StmtList"], ["Stmt"]],
      "default": "::/* default StmtList */"
    },
    {
      "name": "Stmt",
      "rules": [["Expr", "::;\\n"]],
      "default": "::/* default Stmt */"
    },
    {
      "name": "Expr",
      "rules": [["Expr", "Operator", "Expr"], ["Value"]],
      "default": "::12"
    },
    {
      "name": "Value",
      "rules": [["::1"], ["::\"String\""], ["::true"], ["::3.", "::141"]],
      "default": "::1"
    },
    {
      "name": "Operator",
      "rules": [
        ["::+"],
        ["::-"],
        ["::*"],
        ["::/"],
        ["::%"],
        ["::=="],
        ["::!="],
        ["::<"],
        ["::>"],
        ["::<="],
        ["::>="],
        ["::&&"],
        ["::||"]
      ],
      "default": "::+"
    }
  ]
}
